{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "nicColl2016": {
      "type": "object",
      "metadata": {
        "description": "Collection of network interface names."
      }
    },
    "location": {
      "type": "string",
      "metadata": {
        "description": "Azure region."
      }
    },
    "subnetAddsRef": {
      "type": "string",
      "metadata": {
        "description": "ADDS subnet resource id."
      }
    },
    "a16PrivateIps": {
      "type": "object",
      "metadata": {
        "description": "Private IP prefix for DCs"
      }
    }
  },
  "variables": {
    "a1601PrivateIp": "[parameters('a16PrivateIps').a1601privIp]",
    "a1602PrivateIp": "[parameters('a16PrivateIps').a1602privIp]"
  },
  "resources": [
    {
      "comments": "11.14.02.createNetworkInterfaces.a16s01",
      "name": "[parameters('nicColl2016').a1601nic.name]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[parameters('location')]",
      "apiVersion": "2017-10-01",
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAddress": "[variables('a1601PrivateIp')]",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[parameters('subnetAddsRef')]"
              }
            }
          }
        ],
        "dnsSettings": {
          "dnsServers": []
        },
        "enableIPForwarding": false
      }
    },
    {
      "comments": "11.14.02.createNetworkInterfaces.a16s02",
      "name": "[parameters('nicColl2016').a1602nic.name]",
      "type": "Microsoft.Network/networkInterfaces",
      "location": "[parameters('location')]",
      "apiVersion": "2017-10-01",
      "properties": {
        "ipConfigurations": [
          {
            "name": "ipconfig1",
            "properties": {
              "privateIPAddress": "[variables('a1602PrivateIp')]",
              "privateIPAllocationMethod": "Static",
              "subnet": {
                "id": "[parameters('subnetAddsRef')]"
              }
            }
          }
        ],
        "dnsSettings": {
          "dnsServers": []
        },
        "enableIPForwarding": false
      }
    }
  ],
  "outputs": {
    "a16NicIds": {
      "value": [
        "[resourceId('Microsoft.Network/networkInterfaces',parameters('nicColl2016').a1601nic.name)]",
        "[resourceId('Microsoft.Network/networkInterfaces',parameters('nicColl2016').a1602nic.name)]"
      ],
      "type": "array"
    }
  }
}
